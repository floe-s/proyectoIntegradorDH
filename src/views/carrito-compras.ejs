<body class="body">

  <header><%-include('partials/header.ejs') %> </header>
  
<main>

<div class="marco"><a name="title-shop-cart"></a>
  <div class="container">
    <div class="contain-hed">
    <div class="title">Carrito de Compras</div>
    <div href="/" class="carrito-img" href=""></div>
    </div>
      <div class="container-products">
          <div class="object1">
              <div class="trash-obj1"></div>
              <div class="logo-obj1"></div>
              <div class="info">
                <div class="c">Curso de inglés</div>
                <div class="n">Nivel Básico</div>
                <div class="p">Profesor: Jon Snow</div>
                <div class="price">$3.500.00</div>
              </div>  
          </div>

          <div class="object2">
              <div class="trash-obj2"></div>
              <div class="logo-obj2"></div>
              <div class="info2">
                <div class="c2">Curso de Portugués</div>
                <div class="n2">Nivel Avanzado</div>
                <div class="p2">Profesor: Ruy Da Silva</div>
                <div class="price2">$5.800.00</div>
              </div>
          </div>
      </div>
    <div class="cont-tot">
      <div class="totals">
    <ul class="totals-list">
      <li>Subtotal</li>
      <li>$9.300.00</li>
      <li>Total</li>
      <li>$9.300.00</li>
    </ul>
    </div>
    </div>

    <div class="cont-butt">
      <a class="cuotas">O hasta 6 cuotas de $1.550.00</a>
    <button class="comprar" type="button" placeholder="Comprar">Comprar</button>
    </div>
  </div>
</div>

</main>

